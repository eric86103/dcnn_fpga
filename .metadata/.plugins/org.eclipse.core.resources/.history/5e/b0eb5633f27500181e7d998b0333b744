	alt_up_rs232_dev* ptr = malloc(sizeof(alt_up_rs232_dev));
	alt_u8 data;
	alt_u8* data_ptr =malloc(sizeof(alt_u8));
	alt_u8 par;
	alt_u8* par_ptr =malloc(sizeof(alt_u8));

	unsigned int size = 64000;
	int array[64000]={0};
	int i=0;
	int a = 1;
	while(1){
